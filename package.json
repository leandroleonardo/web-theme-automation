{
  "name": "cronapp-qa-cypress",
  "version": "1.0.0",
  "description": "Testes automatizados com Cypress - Cronapp",
  "main": "index.js",
  "scripts": {
    "clean:reports": "if exist cypress\\reports rmdir /s/q cypress\\reports && mkdir cypress\\reports && mkdir cypress\\reports\\mochareports",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports --inline",
    "posttest": "npm run combine-reports && npm run generate-report",
    "test": "npm run scripts || npm run posttest",
    "visual-test": "cypress open"
  },
  "author": "Felipe Santos dos Anjos",
  "license": "ISC",
  "devDependencies": {
    "chai-colors": "^1.0.1",
    "cypress": "^10.11.0",
    "cypress-multi-reporters": "^1.6.3",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CronApp/cronapp-qa-automation.git"
  },
  "bugs": {
    "url": "https://github.com/CronApp/cronapp-qa-automation/issues"
  },
  "homepage": "https://github.com/CronApp/cronapp-qa-automation#readme"
}